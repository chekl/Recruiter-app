<?xml version="1.0" encoding="UTF-8" ?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Old_Reviewed_Date</fullName>
    <active>true</active>
    <errorConditionFormula>Reviewed_Date__c &lt; Today()</errorConditionFormula>
    <errorDisplayField>Reviewed_Date__c</errorDisplayField>
    <errorMessage>Reviewed date cannot be in the past</errorMessage>
</ValidationRule>
